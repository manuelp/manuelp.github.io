<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="https://www.w3.org/2005/Atom">
 
 <title>Manuel Paccagnella</title>
 <link href="https://manuelp.github.com/atom.xml" rel="self"/>
 <link href="https://manuelp.github.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>https://manuelp.github.com/</id>
 <author>
   <name>Manuel Paccagnella</name>
   <email>manuel.paccagnella@gmail.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="https://manuelp.github.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>https://manuelp.github.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>